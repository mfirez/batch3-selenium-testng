<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Eurotech Automation" verbose="1">
    <test name="Eurotech Smoke Test">
        <packages>
            <package name="com.eurotech.tests.day26.ProjectObjectModel"></package>
            <package name="com.eurotech.tests.day22.webTables.HoverTest"></package>
            
        </packages>
        
        <classes>
            <class name="com.eurotech.tests.day22.webTables.HoverTest">
                <methods>
                    <include name="printAllCellByIndex"></include>
                </methods>
